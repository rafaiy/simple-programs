import javax.swing.*;
public class joptionpane {
	public static void main(String args[]){
		
	String s=	JOptionPane.showInputDialog("insert the number","you can only enter numbers");
	String s2 = JOptionPane.showInputDialog("enter second no","you can only enter numbers");
	JOptionPane.showMessageDialog(null,s+s2 , "this is the title",JOptionPane.PLAIN_MESSAGE);
	int a = Integer.parseInt(s);
	int b = Integer.parseInt(s2);
		
	}

}
