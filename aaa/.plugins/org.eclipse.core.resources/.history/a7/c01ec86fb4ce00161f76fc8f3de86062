
public class fffinalvariable {
	private int any = 5;
	private final int FFF;
	
	public fffinalvariable(int x){
		
		FFF = x;
		any+=FFF;
	}
	public String toString(){
		
		return String.format("this is the final value of the varubale::::::: i.e %s and the value of any is :: %s ",FFF,any);
		
	}

}
